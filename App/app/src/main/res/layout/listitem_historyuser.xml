<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="50dp"
    android:layout_marginTop="15dp">

    <androidx.cardview.widget.CardView
        android:layout_width="320dp"
        android:layout_height="200dp"
        app:cardCornerRadius="10dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent">

        <TextView
            android:id="@+id/tvUsernameLdr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="USERNAME"
            android:textColor="#20466F"
            android:textSize="18sp"
            android:textStyle="bold"
            android:layout_marginStart="20dp"
            android:layout_marginTop="10dp"
            />

        <TextView
            android:id="@+id/tglHistoryUsr"
            android:textColor="#000000"
            android:text="tanggal"
            android:textSize="14sp"
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginStart="210dp"/>

        <TextView
            android:id="@+id/tvAlamatLdr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="alamat laundry"
            android:textColor="#000000"
            android:textSize="12sp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="40dp"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Cuci Kering"
            android:textColor="#20466F"
            android:textStyle="bold"
            android:layout_marginStart="20dp"
            android:layout_marginTop="90dp"/>

        <TextView
            android:id="@+id/tvJenisCucian1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:textColor="#050505"
            android:layout_marginStart="20dp"
            android:layout_marginTop="110dp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="KG"
            android:textColor="#050505"
            android:layout_marginStart="38dp"
            android:layout_marginTop="110dp"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Cuci Setrika"
            android:textColor="#20466F"
            android:textStyle="bold"
