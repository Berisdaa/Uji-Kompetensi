            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Setrika"
            android:textColor="#20466F"
            android:textStyle="bold"
            android:layout_marginStart="170dp"
            android:layout_marginTop="150dp"/>
